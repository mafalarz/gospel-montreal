{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/francais",
      "rewrite": "/index.fr.html"
    },
    {
      "route": "/english",
      "rewrite": "/index.html"
    }
  ],
  "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
  }
}